{
  "manifest_version": 2,
  "name": "PhishFort | Crypto",
  "description": "Protect against phishing attacks.",
  "version": "0.0.1",
  "author": "PhishFort",
  "browser_action": {
    "default_icon": "images/tab-icon.png",
    "default_icon": "images/tab-icon.png",
    "default_title": "PhishFort | Crypto",
    "default_popup": "index.html"
  },
  "icons": {
    "128": "images/tab-icon.png"
  },
  "background": {
    "scripts": [
      "js/jquery-3.3.1.min.js",
      "js/background.js"
    ],
    "persistent": false
  },
  "web_accessible_resources": [
    "warning.html"
  ],
  "content_security_policy": "script-src 'self' https://www.google.com https://www.gstatic.com; object-src 'self'",
  "permissions": [
    "tabs"
  ]
}